events {
        worker_connections 768;
}

http {

       server {
               listen 192.168.56.60:{{ nginx_listen_port }};

	       root /usr/share/nginx/html;
	       index index.html index.htm;

	       server_name otushost.local;

	       location / {
        	       try_files $uri $uri/ =404;
      	       }

       }

}

